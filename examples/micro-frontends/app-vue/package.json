{
  "name": "@mf/app-vue",
  "version": "0.0.0",
  "description": "app-vue description",
  "private": true,
  "scripts": {
    "dev": "fbi serve -p 9002",
    "build": "fbi build",
    "lint": "eslint src --ext js,ts,vue --fix",
    "format": "prettier -c --write \"./**/*.{js,ts,md,html,css,vue}\""
  },
  "dependencies": {
    "single-spa-vue": "^2.1.0",
    "systemjs-webpack-interop": "^2.3.1",
    "vue": "^2.6.12",
    "vue-router": "^3.4.9"
  },
  "devDependencies": {
    "@fbi-js/factory-web": "file:../../../",
    "@typescript-eslint/parser": "^4.9.1",
    "@vue/eslint-config-typescript": "^7.0.0",
    "eslint-plugin-vue": "^7.2.0",
    "vue-loader": "^15.9.5",
    "vue-template-compiler": "^2.6.12"
  },
  "fbi": {
    "factory": {
      "id": "@fbi-js/factory-web",
      "version": "",
      "template": "micro-vue",
      "features": {
        "typescript": true
      }
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/typescript"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "ignorePatterns": [],
    "rules": {
      "semi": [
        "error",
        "never"
      ]
    }
  },
  "postcss": {
    "plugins": [
      "postcss-preset-env"
    ]
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "all",
    "endOfLine": "auto"
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-prettier"
    ],
    "plugins": [],
    "ignoreFiles": [],
    "rules": {}
  }
}
