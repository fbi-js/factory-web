{
  "name": "app-react",
  "version": "0.0.0",
  "description": "app-react description",
  "private": true,
  "scripts": {
    "dev": "fbi serve -p 9003",
    "build": "fbi build",
    "lint": "eslint src --ext js,jsx,ts,tsx --fix",
    "format": "prettier -c --write \"./**/*.{js,jsx,ts,tsx,md,html,css}\""
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "single-spa-react": "^3.2.0",
    "systemjs-webpack-interop": "^2.3.1"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.12.7",
    "@fbi-js/factory-web": "file:../../../",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/single-spa-react": "^3.0.0",
    "eslint-config-react-app": "^6.0.0",
    "react-hot-loader": "^4.13.0"
  },
  "fbi": {
    "factory": {
      "id": "@fbi-js/factory-web",
      "version": "",
      "template": "micro-react",
      "features": {
        "typescript": true
      }
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript",
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {
      "semi": [
        "error",
        "never"
      ]
    }
  },
  "postcss": {
    "plugins": [
      "postcss-preset-env"
    ]
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "all",
    "endOfLine": "auto"
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-prettier"
    ],
    "plugins": [],
    "ignoreFiles": [],
    "rules": {}
  }
}
