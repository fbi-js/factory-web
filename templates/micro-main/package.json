{
  "name": "<%_ if (project.orgName) { _%>@<%= project.orgName %>/<%_ } _%><%= project.name %>",
  "version": "0.0.0",
  "description": "<%= project.description %>",
  "private": true,
  "main": "dist/main.js",
  "scripts": {
    "dev": "fbi serve",
    "build": "fbi build",
    "lint": "npx eslint --ext .vue,.js --fix",
    "format": "npx prettier -c --write \"**/*.{js,md,html,css,less}\""
  },
  "dependencies": {
    "single-spa": "^5.8.2",
    "axios": "^0.21.0"
  },
  "devDependencies": {
    "@fbi-js/factory-web": "~1",
    "@types/systemjs": "^6.1.0"
  },
  "fbi": {
    "factory": {
      "id": "<%= factory.id %>",
      "version": "<%= factory.version %>",
      "template": "<%= factory.template %>",
      "features": <%- JSON.stringify(project.features) %>
    }
  }
}
