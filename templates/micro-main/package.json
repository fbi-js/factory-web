{
  "name": "<%_ if (project.orgName) { _%>@<%= project.orgName %>/<%_ } _%><%= project.name %>",
  "version": "0.0.0",
  "description": "<%= project.description %>",
  "private": true,
  "main": "dist/main.js",
  "scripts": {
    "dev": "fbi serve",
    "build": "fbi build",
    "lint": "eslint --ext js,ts --fix",
    "format": "prettier -c --write \"**/*.{js,ts,md,html,css}\""
  },
  "dependencies": {
    "single-spa": "^5.8.3",
    "axios": "^0.21.0"
  },
  "devDependencies": {
    "@fbi-js/factory-web": "^<%= factoryVersion %>",
    "@fbi-js/tsconfig": "^0.0.1",
    "@types/systemjs": "^6.1.0",
    "typescript": "^4.1.3"
  },
  "fbi": {
    "factory": {
      "id": "<%= factory.id %>",
      "version": "<%= factory.version %>",
      "template": "<%= factory.template %>",
      "features": <%- JSON.stringify(project.features) %>
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "none",
    "endOfLine": "auto"
  }
}
