{
  "name": "<%= project.name %>",
  "version": "0.0.0",
  "description": "<%= project.description %>",
  "private": true,
  "scripts": {
    "dev": "fbi serve -p 9003",
    "build": "fbi build",
    "lint": "npx eslint --ext .vue,.js --fix",
    "format": "npx prettier -c --write \"**/*.{js,md,html,css,less}\"",
    "postinstall": "fbi install"
  },
  "dependencies": {
    "single-spa": "5.5.5"
  },
  "devDependencies": {
    "@fbi-js/factory-web": "*"
  },
  "fbi": {
    "factory": {
      "id": "<%= factory.id %>",
      "version": "<%= factory.version %>",
      "template": "<%= factory.template %>",
      "features": <%- JSON.stringify(project.features) %>
    }
  }
}
