{
  "name": "<%= project.name %>",
  "version": "0.0.0",
  "description": "<%= project.description %>",
  "private": true,
  "scripts": {
    "dev": "fbi serve",
    "build": "fbi build",
    "lint": "npx eslint --ext .jsx,.js --fix"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.12.7",
    "@fbi-js/factory-web": "~1",
    <%_ if (project.features.typescript) { _%>
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    <%_ } _%>
    "react-hot-loader": "^4.13.0"
  },
  "fbi": {
    "factory": {
      "id": "<%= factory.id %>",
      "version": "<%= factory.version %>",
      "template": "<%= factory.template %>",
      "features": <%- JSON.stringify(project.features) %>
    }
  }
}
