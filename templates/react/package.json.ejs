{
  "name": "my-app",
  "version": "1.0.0",
  "description": "my-app description",
  "private": true,
  "scripts": {
    "dev": "fbi serve",
    "build": "fbi build",
    "format": "fbi format"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@fbi-js/factory-web": "^2.0.0-alpha.3",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "typescript": "^4.1.3"
  },
  "fbi": {
    "factory": {
      "id": "<%= factory.id %>",
      "template": "<%= factory.template %>",
      "features": <%= project.features %>
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "eslintConfig": {
    "extends": [
      "@fbi-js/react-typescript"
    ],
    "rules": {
      "semi": [
        2,
        "never"
      ],
      "jsx-quotes": [
        "error",
        "prefer-double"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "space-before-function-paren": [
        "error",
        "never"
      ]
    }
  },
  "stylelint": {
    "extends": "@fbi-js/stylelint-config"
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "none"
  }
}
