{
  "name": "<%_ if (project.orgName) { _%>@<%= project.orgName %>/<%_ } _%><%= project.name %>",
  "version": "0.0.0",
  "description": "<%= project.description %>",
  "private": true,
  "scripts": {
    "dev": "fbi serve<% if (project.isMicro) { %> -p 9001<% } %>",
    "build": "fbi build",
    "format": "fbi format"
  },
  "dependencies": {
    <%_ if (project.features.admin) { _%>
    "@ant-design/icons": "^4.4.0",
    "antd": "^4.11.2",
    "axios": "^0.21.1",
    "react-router-dom": "^5.2.0",
    <%_ } _%>
    <%_ if (project.isMicro) { _%>
    "single-spa-react": "^3.2.0",
    "systemjs-webpack-interop": "^2.3.4",
    <%_ } _%>
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    <%_ if (project.features.typescript) { _%>
    "<%= factory.id %>": "^<%= factoryVersion %>",
    <%_ } else { _%>
    "<%= factory.id %>": "^<%= factoryVersion %>"
    <%_ } _%>
    <%_ if (project.features.typescript) { _%>
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    <%_ if (project.features.admin) { _%>
    "@types/react-router-dom": "^5.1.7",
    <%_ } _%>
    <%_ if (project.isMicro) { _%>
    "@types/single-spa-react": "^3.0.0",
    <%_ } _%>
    "typescript": "^4.1.3"
    <%_ } _%>
  },
  "fbi": {
    "factory": {
      "id": "<%= factory.id %>",
      "version": "<%= factory.version %>",
      "template": "<%= factory.template %>",
      "features": <%- JSON.stringify(project.features) %>
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "eslintConfig": {
    "extends": [
      "@fbi-js/react<%_ if (project.features.typescript) { _%>-typescript<%_ } _%>"
    ]
  },
  "stylelint": {
    "extends": "@fbi-js/stylelint-config"
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "endOfLine": "lf",
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "none",
    "arrowParens": "always",
    "vueIndentScriptAndStyle": true
  }
}
