{
  "name": "<%= project.name %>",
  "version": "0.0.0",
  "description": "<%= project.description %>",
  "private": true,
  "scripts": {
    "dev": "fbi serve",
    "build": "fbi build",
    "format": "fbi format"
  },
  "dependencies": {
    "vue": "^2.6.12",
    "vue-router": "^3.4.9"
  },
  "devDependencies": {
    <%_ if (project.features.typescript) { _%>
    "typescript": "^4.1.3",
    <%_ } _%>
    "<%= factory.id %>": "^<%= factoryVersion %>"
  },
  "fbi": {
    "factory": {
      "id": "<%= factory.id %>",
      "version": "<%= factory.version %>",
      "template": "<%= factory.template %>",
      "features": <%- JSON.stringify(project.features) %>
    }
  },
  "stylelint": {
    "extends": ["@fbi-js/stylelint-config"]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "eslintConfig": {
    "extends": [
      "@fbi-js/vue<%_ if (project.features.typescript) { _%>-typescript<%_ } _%>"
    ]
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "endOfLine": "lf",
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "none",
    "arrowParens": "always",
    "vueIndentScriptAndStyle": true
  }
}
