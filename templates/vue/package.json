{
  "name": "<%_ if (project.orgName) { _%>@<%= project.orgName %>/<%_ } _%><%= project.name %>",
  "version": "0.0.0",
  "description": "<%= project.description %>",
  "private": true,
  "scripts": {
    "dev": "fbi serve <% if (project.isMicro) { %>-p 9001<% } %>",
    "build": "fbi build",
    "lint": "eslint src --ext js,<% if (project.features.typescript) { %>ts,<% } %>vue --fix",
    "format": "prettier -c --write \"./**/*.{js,<% if (project.features.typescript) { %>ts,<% } %>md,html,css,vue}\""
  },
  "dependencies": {
    <%_ if (project.isMicro) { _%>
    "single-spa-vue": "^2.1.0",
    "systemjs-webpack-interop": "^2.3.4",
    <%_ } _%>
    "vue": "^2.6.12",
    "vue-router": "^3.4.9"
  },
  "devDependencies": {
    <%_ if (project.features.typescript) { _%>
    "typescript": "^4.1.3",
    <%_ } _%>
    "@fbi-js/factory-web": "^<%= factoryVersion %>"
  },
  "fbi": {
    "factory": {
      "id": "<%= factory.id %>",
      "version": "<%= factory.version %>",
      "template": "<%= factory.template %>",
      "features": <%- JSON.stringify(project.features) %>
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "eslintConfig": {
    "extends": [
      "@fbi-js/vue<%_ if (project.features.typescript) { _%>-typescript<%_ } _%>"
    ]
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "none",
    "endOfLine": "auto"
  }
}
