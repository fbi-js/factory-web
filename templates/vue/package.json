{
  "name": "<%= project.name %>",
  "version": "0.0.0",
  "description": "<%= project.description %>",
  "private": true,
  "scripts": {
    "dev": "fbi serve  <%_ if (project.isMicro) { _%> <%=  %> -p 9001 <%_ } _%>",
    "build": "fbi build",
    "lint": "npx eslint --ext .vue,.js --fix",
    "postinstall": "fbi install"
  },
  "dependencies": {
    <%_ if (project.isMicro) { _%>
    "single-spa-vue": "^2.1.0",
    "systemjs-webpack-interop": "^2.3.0",
    <%_ } _%>
    "vue": "^2.6.12",
    "vue-router": "^3.4.9"
  },
  "devDependencies": {
    "@fbi-js/factory-web": "~1"
  },
  "fbi": {
    "factory": {
      "id": "<%= factory.id %>",
      "version": "<%= factory.version %>",
      "template": "<%= factory.template %>",
      "features": <%- JSON.stringify(project.features) %>
    }
  }
}
