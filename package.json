    "webpack-config-single-spa-ts": "^1.18.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fbi-js/factory-web.git"
  },
  "homepage": "https://github.com/fbi-js/factory-web",
  "bugs": {
    "url": "https://github.com/fbi-js/factory-web/issues"
