    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.0.1",
    "css-minimizer-webpack-plugin": "^1.1.5",
    "ejs": "^3.1.5",
    "eslint": "^7.16.0",
    "eslint-webpack-plugin": "^2.4.1",
    "fbi": "^4.3.2",
    "fibers": "^5.0.0",
    "fork-ts-checker-webpack-plugin": "^6.0.8",
